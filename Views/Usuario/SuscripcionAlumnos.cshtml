@model Pago


<h1>Ultima suscripción registrada</h1>

<div class="form row justify-content-center">
    <div class="col-md-4">

        <div>
            <p>Estado de mi suscripción: @ViewBag.EstadoSuscripcion </p> 
        </div>



        <div>
            <p>Ultimo pago registrado: @ViewBag.UltimoPagoRegistrado</p>
            
        </div>


        <div>
            <p>Ultimo mes activo: @ViewBag.UltimoMesActivo</p>
            
        </div>


        <form>
            <div class="d-flex justify-content-center">
                <button class="btn btn-outline-primary" asp-action="InicioAlumno">Volver</button>
            </div>
        </form>

        


    </div>
</div>
